{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#E06C75",
    "template": "❯ "
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "time_format": "15:04"
          },
          "template": "[{{ .CurrentDate | date .Format }}]",
          "foreground": "#E5C07B",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#F3C267",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "template": " x{{ reason .Code }}",
          "foreground": "#b8ff75",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "style": "roundrock"
          },
          "template": " {{ .FormattedMs }} ",
          "foreground": "#b8ff75",
          "type": "executiontime",
          "style": "diamond",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "style": "full"
          },
          "template": "\ue5ff {{ .Path }}",
          "foreground": "#61AFEF",
          "type": "path",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "!",
          "foreground": "#E06C75",
          "type": "root",
          "style": "plain"
        },
        {
          "template": "❯",
          "foreground": "#E06C75",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
